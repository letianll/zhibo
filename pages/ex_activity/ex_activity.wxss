page{
  font-family:"Microsoft YaHei";
}
.swiper-tab{ 
  width: 100%; 
  border-bottom: 2rpx solid #eeeeee; 
  text-align: center; 
  line-height: 80rpx;
  position: fixed; top:0px; z-index: 10000; background-color: #fff; height: 80rpx;} 
.swiper-tab-item{ font-size: 30rpx; 
  display: inline-block; 
  width: 50%; 
  color: #666666; 
}
.on{ color: #ff9933; 
  border-bottom: 5rpx solid #ff9933;} 

.swiper-box{ display: block; height: 80%; width: 100%; overflow: hidden; margin-top: 14%; } 

.showimg{
  width: 100%; height: 50rpx; line-height: 50rpx;
}
.showimg image{
  width:50rpx; height: 50rpx; 
}
.img_view{
 float: right; margin-right: 2px; margin-bottom:2px;font-size: 13px;display: flex; 
}
.img_view text{
   font-size: 16px;
   margin-right: 5px;
}
