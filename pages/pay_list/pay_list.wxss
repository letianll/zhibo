.showimg{
  width: 100%; height: 40px;
}
.showimg image{
  width:30px; height: 30px; float: right; margin-right: 10px;
}
.table {
   font-size: 14px;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: auto;
  align-items: center;
  padding-top:10rpx;
  padding-bottom: 10rpx;
}
.td {
    width:20%;
    justify-content: center;
    text-align: center;
}
.td1{
    width:30%;
    justify-content: center;
    text-align: center;
}
.td2{
    width:50%;
    justify-content: center;
    text-align: center;
    overflow: hidden;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #fffee5;
}
.th {
  width: 20%;
  justify-content: center;
  background: #ff9933;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}
.th1{
  width: 30%;
  justify-content: center;
  background: #ff9933;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}
.th2{
  width: 50%;
  justify-content: center;
  background: #ff9933;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}
.td button{
  width:50%; height: 60%; color: #fff; background-color: rgb(51,102,255); font-size: 14px;
}